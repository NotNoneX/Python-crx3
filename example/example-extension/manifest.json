{
  "name": "CRX3 Example Extension",
  "version": "1.0.0",
  "description": "An example extension for https://github.com/liying2008/python-crx3",
  "permissions": [
    "browsingData",
    "alarms"
  ],
  "action": {
    "default_icon": "img/icon.png",
    "default_popup": "popup/popup.html"
  },
  "icons": {
    "128": "img/icon.png"
  },
  "background": {
    "service_worker": "background/bg-wrapper.js"
  },
  "manifest_version": 3
}
